            <w:p wsp:rsidR="00DE6655" wsp:rsidRPr="00586700" wsp:rsidRDefault="00DE6655" wsp:rsidP="00DE6655">
				<w:pPr>
					<w:jc w:val="left"/>
					<w:rPr>
						<w:rFonts w:ascii="宋体" w:fareast="宋体" w:h-ansi="宋体"/>
